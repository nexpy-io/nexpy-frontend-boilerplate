    "@xstyled/styled-components": "^3.3.0",
    "@xstyled/system": "^3.2.0",
    "axios": "^0.26.0",
    "lodash": "^4.17.21",
    "next": "12.1.0",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.4.4",
    "nookies": "^2.5.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
