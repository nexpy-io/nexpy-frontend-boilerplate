{
  "name": "nexpy-frontend-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "reinstall": "yarn concurrently \"rm -rf ./node_modules\" \"rm -rf ./yarn.lock ./package-lock.json ./.next ./storybook-static\" && yarn",
    "dev": "next dev",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "commit": "cz",
    "type:check": "tsc --project tsconfig.json --pretty --noEmit",
    "prettier": "prettier --loglevel warn --write .",
    "prettier:check": "prettier --check .",
    "eslint": "eslint --fix --max-warnings 0 .",
    "eslint:check": "eslint --max-warnings 0 .",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.8.8",
    "@mdx-js/loader": "^2.0.0",
    "@nexpy/react-easy-context-api": "^1.0.2",
